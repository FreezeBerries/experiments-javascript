doctype html
html(lang='en')
  head
    meta(charset='utf-8')
    meta(name='viewport', content='width=device-width, initial-scale=1, shrink-to-fit=no')
    //meta(name='description', content='')
    //meta(name='author', content='Mark Otto, Jacob Thornton, and Bootstrap contributors')
    //meta(name='generator', content='Jekyll v3.8.5')
    title Carousel Template · Bootstrap
    //link(rel='canonical', href='https://getbootstrap.com/docs/4.3/examples/carousel/')
    link(type='text/css', rel='stylesheet', href='/lib/css/bootstrap.min.css')

    //link(rel='stylesheet', href='/css/site.css')
    block head

    style.
      .bd-placeholder-img {
      font-size: 1.125rem;
      text-anchor: middle;
      -webkit-user-select: none;
      -moz-user-select: none;
      -ms-user-select: none;
      user-select: none;
      }
      @media (min-width: 768px) {
      .bd-placeholder-img-lg {
      font-size: 3.5rem;
      }
      }

    // Custom styles for this template
    link(href='/css/carousel.css', rel='stylesheet')
  body
    header
      nav.navbar.navbar-expand-md.navbar-dark.bg-dark
        a.navbar-brand(href='/') #{company.name}
        button.navbar-toggler(type='button', data-toggle='collapse', data-target='#navbarCollapse', aria-controls='navbarCollapse', aria-expanded='false', aria-label='Toggle navigation')
          span.navbar-toggler-icon
        #navbarCollapse.collapse.navbar-collapse
          ul.navbar-nav.mr-auto
            li.nav-item(class= sidebar === "home" ? "active" : "")
              a.nav-link.waves-effect(href='/') Home
            li.nav-item(class= sidebar === "gallery" ? "active" : "")
              a.nav-link.waves-effect(href='/gallery') Gallery
            li.nav-item(class= sidebar === "projects" ? "active" : "")
              a.nav-link.waves-effect(href='/projects') Projects
            li.nav-item(class= sidebar === "services" ? "active" : "")
              a.nav-link.waves-effect(href='/services') Services
          //form.form-inline.mt-2.mt-md-0
            input.form-control.mr-sm-2(type='text', placeholder='Search', aria-label='Search')
            button.btn.btn-outline-success.my-2.my-sm-0(type='submit') Search
    .container-fuild
      block content
      
    footer.container-fuild.bg-dark.text-light.pb-4.pt-2
      .row-fuild
        .col-sm-12
          p.float-right
            a(href='/admin/dashboard') Admin Login
          p
            | © 2017-2019 #{company.name}
            //a(href='#') Privacy
            //|  · 
            //a(href='#') Terms
          p 
            | Phone: 
            a(href="tel:"+company.phone) #{company.phone}
            | &nbsp; Email:
            a(href="mailto:"+company.email) #{company.email}
            br
            if company.facebook_link 
              a(href=company.facebook_link) Facebook
              span &nbsp;|&nbsp;
            if company.yelp_link
              a(href=company.yelp_link) Yelp
              span &nbsp;|&nbsp;
            if company.angies_list_link 
              a(href=company.angies_list_link) Angie's List
              span &nbsp;|&nbsp;
            if company.youtube_link 
              a(href=company.youtube_link) Youtube
              span &nbsp;|&nbsp;
            if company.twitter_link 
              a(href=company.twitter_link) Twitter
              span &nbsp;|&nbsp;
              
                

            
    script(src='/js/lib/jquery.min.js')
    script(src='/lib/js/bootstrap.min.js')
    // script(src='/js/lib/materialize.min.js')
    block scripts
    //script(src='https://code.jquery.com/jquery-3.3.1.slim.min.js', integrity='sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo', crossorigin='anonymous')
